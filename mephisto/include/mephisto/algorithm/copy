#ifndef MEPHISTO_COPY
#define MEPHISTO_COPY
#include <mephisto/type_traits>


namespace mephisto {

template <
  typename StreamT,
  typename InBufT,
  typename OutBufT> 
std::enable_if<std::is_same<HostDevice, typename buf_traits<InBufT>::DeviceType>::value>
copy(StreamT &streamT, InBufT &inBuf, OutBufT &outBuf) {
}
}

#endif
